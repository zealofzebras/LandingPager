@model IEnumerable<LandingPager.Models.LandingFeature>

<section class="features">
    <div class="container text-center">

        <h4>All our features (from RCL)</h4>
        <div class="row">
            <div class="col-md-4">
                <ul>
                    @foreach (var feature in Model)
                    {
                        <li><a asp-area="Landing" asp-page="/FeaturePage" asp-route-title="@feature.Title">@feature.Title</a></li>
                    }
                </ul>
            </div>

        </div>
    </div>
</section>