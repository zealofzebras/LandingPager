@model IEnumerable<LandingPager.Models.LandingCompetitor>

<section class="features">
    <div class="container text-center">

        <h4>Compare to (from RCL)</h4>
        <div class="row">
            <div class="col-md-4">
                <ul>
                    @foreach (var competitor in Model)
                    {
                        <li><a asp-area="Landing" asp-page="/CompetitorPage" asp-route-title="@competitor.Name">@competitor.Name</a></li>
                    }
                </ul>
            </div>

        </div>
    </div>
</section>