@page "{title?}"
@model LandingPager.Blog.Pages.IndexModel


@{
    if (Model.BlogPost != null)
    {
        ViewData["Title"] = Model.BlogPost.Title;
        @await Html.PartialAsync("BlogPost", Model.BlogPost)
    }
    else
    {
        ViewData["Title"] = "Blog";
        @await Html.PartialAsync("BlogIndex", Model.BlogPosts)
    }
}




